class Queue:
    def __init__(self):
        #
        # Write code here
        #
        self.Que = []
        
    def put(self, elem):
        #
        # Write code here
        #
        self.Que.append(elem)
        print(elem)
        
    def get(self):
        #
        # Write code here
        #
        print(Que[0])
        for i in range(len(Que)): #Reading until i-1
            Que [i] = Que[i + 1]
            del Que [i+1]
    
    def __str__(self):
        return(self.lst)

que = Queue()
que.put(1)
que.put("dog")
que.put(False)


try:
    for i in range(4):
        print(que.get())
except:
    print("Queue error")

