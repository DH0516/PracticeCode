class QueueError(IndexError):  # Choose base class for the new exception.
    #
    #  Write code here
    #
    pass

class Queue:
    def __init__(self):
        #
        # Write code here
        #
        self.Que = []
        
    def put(self, elem):
        #
        # Write code here
        #
        self.Que.insert(0, elem)
        
    def get(self):
        #
        # Write code here
        #
        if len(self.Que) > 0:
            elem = self.Que[-1]
            del self.Que[-1]
            return elem
        else:
            return QueueError
    
    def __str__(self):
        return(str(self.Que))

que = Queue()
que.put(1)
que.put("dog")
que.put(False)

for i in range(4):
    print(que.get())

